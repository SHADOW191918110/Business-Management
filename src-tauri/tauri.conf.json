{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "POS-App",
  "version": "1.0.0",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "POS-App",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "decorations": true
      }
    ]
  },
  "bundle": {
    "identifier": "net.example.posapp"
  },
  "plugins": {}
}